.movie-list {
  box-shadow: 0 8rpx 17rpx 0 rgba(7, 17, 27, 0.1);
  margin: 0 25rpx;
  overflow: hidden;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  background: #fff;
}

.video-des {
  padding: 10px;
}

.myVideo {
 
  width: 100%;
}

.video_title {
  display: block;
  font-size: 15px;
  overflow: hidden;
  white-space: nowrap;
  color: #333;
  text-overflow: ellipsis;
}

.video_time {
  font-size: 13px;
  color: #979797;
}
